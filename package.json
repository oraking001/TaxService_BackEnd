    "@sendgrid/mail": "^7.6.0",
    "aws-sdk": "^2.1048.0",
    "axios": "^0.21.4",
    "bcryptjs": "^2.4.3",
    "client": "file:client",
    "config": "^3.3.3",
    "express": "^4.17.1",
    "express-fileupload": "^1.3.1",
    "express-validator": "^6.8.1",
    "gravatar": "^1.8.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.8",
    "mysql": "^2.18.1",
    "nodemailer": "^6.7.2",
    "normalize-url": "^5.3.0",
    "web3": "^1.6.1",
